{
  "name": "@terenceodonoghue/react-icons",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/terenceodonoghue/icons.git",
    "directory": "packages/react-icons"
  },
  "author": "Terence O'Donoghue <1697905+terenceodonoghue@users.noreply.github.com>",
  "type": "module",
  "exports": {
    "./fantasticons": {
      "import": "./dist/mjs/fantasticons/index.js",
      "require": "./dist/cjs/fantasticons/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "fantasticons": [
        "./dist/mjs/fantasticons/index.d.ts",
        "./dist/cjs/fantasticons/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path ../../.eslintignore --max-warnings=0 ./",
    "prepack": "node scripts/generate.js && yarn concurrently \"yarn:prepack:*\"",
    "prepack:cjs": "tsc -p tsconfig-cjs.json",
    "prepack:mjs": "tsc -p tsconfig-mjs.json",
    "prepare": "rimraf ./dist \"./out-svgr/*/\""
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@terenceodonoghue/svg-icons": "workspace:packages/svg-icons"
  },
  "devDependencies": {
    "@svgr/core": "^6.2.1",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "lodash-es": "^4.17.21",
    "mem-fs": "^2.2.1",
    "mem-fs-editor": "^9.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "xml2js": "^0.4.23"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
